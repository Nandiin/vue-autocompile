{
  "name": "vue-autocompile",
  "main": "./lib/vue-autocompile",
  "version": "0.0.3",
  "description": "Auto compiles vue in atom",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulpflug/vue-autocompile"
  },
  "activationCommands": {
    "atom-text-editor": [
      "core:save"
    ]
  },
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "vue-compiler": "~0.0.2"
  },
  "consumedServices": {
    "autoreload": {
      "versions": {
        "^0.0.1": "consumeAutoreload"
      }
    },
    "debug": {
      "versions": {
        "^0.0.1": "consumeDebug"
      }
    }
  }
}
